    "license": "MIT",
    "scripts": {
        "build": "rmdir dist /s /q && mkdir dist && babel src -d dist --copy-files --extensions \".ts\" --no-copy-ignored && tsc --p tsconfig.module.json",
        "build:mac": "rm -r dist && mkdir dist && babel src -d dist --copy-files --extensions \".ts\" --no-copy-ignored && tsc -p tsconfig.module.json",
        "lint": "eslint \"src/**/*.{ts,js}\"",
        "lint:fix": "eslint --fix \"src/**/*.{ts,js}\""
    },
    "babel": {
        "presets": [
            "@babel/preset-typescript",
            "@babel/preset-env"
        ]
    },
    "dependencies": {
        "axios": "^0.27.2",
        "dayjs": "^1.11.5",
        "dotenv": "^16.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/preset-env": "^7.20.2",
        "@types/jest": "^28.1.8",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "babel-plugin-named-exports-order": "^0.0.2",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin": "^1.0.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.0.1",
        "prettier": "^2.7.1",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    }
