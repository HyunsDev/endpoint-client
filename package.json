{
    "name": "endpoint-client",
    "version": "0.1.1",
    "description": "endpoint Client",
    "homepage": "https://github.com/HyunsDev/endpoint-client",
    "module": "dist/index.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bugs": {
        "url": "https://github.com/HyunsDev/endpoint-client/issues",
        "email": "hello@opize.me"
    },
    "repository": "https://github.com/HyunsDev/endpoint-client.git",
